#define Num_Samples 53 
int i = 0; 
byte value = 0; 
static byte WaveFormTable[Num_Samples] = {
  0x00, 0x05, 0x09, 0x0E, 0x12, 0x17, 0x1B, 0x20,
  0x24, 0x29, 0x2E, 0x32, 0x37, 0x3B, 0x40, 0x44,
  0x49, 0x4D, 0x52, 0x57, 0x5B, 0x60, 0x64, 0x69, 
  0x6D, 0x72, 0x76, 0x7B, 0x80, 0x84, 0x89, 0x8D,
  0x92, 0x96, 0x9B, 0x9F, 0xA4, 0xA8, 0xAD, 0xB2, 
  0xB6, 0xBB, 0xBF, 0xC4, 0xC8, 0xCD, 0xD1, 0xD6, 
  0xED, 0xF1, 0xF6, 0xFA, 0xFF
};

void setup() {
  Serial.begin(115200);

}

void loop() {
  dacWrite(25, WaveFormTable[i]);
  i++; 
  delay(20);
  Serial.print(" i = ");
  Serial.print(i);
  Serial.print("  ");
  Serial.print("  ||  ");
  value = digitalRead(12);
  if(value == 1){
    Serial.println(" Digital = 50 // HIGH");
  }
  else {
    Serial.println(" Digital = 0 // LOW");

  }

  if(i>= Num_Samples) {
    i=0; 
  }

}
